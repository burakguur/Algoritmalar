    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi, carpanToplami, sayac" type="Integer" array="False" size=""/>
            <output expression="&quot;Bir Say&#305; Giriniz&quot;" newline="True"/>
            <input variable="sayi"/>
            <assign variable="carpanToplami" expression="0"/>
