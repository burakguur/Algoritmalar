                        <assign variable="carpanToplami" expression="carpanToplami + sayac"/>
                    </then>
                    <else/>
                </if>
            </for>
            <if expression="sayi == carpanToplami">
                <then>
                    <output expression="&quot;Say&#305;n&#305;z M&#252;kemmel&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Say&#305;n&#305;z M&#252;kemmel De&#287;il&quot;" newline="True"/>
                </else>
            </if>
        </body>
